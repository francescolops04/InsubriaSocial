<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".Settings">

    <View
        android:id="@+id/view3"
        android:layout_width="477dp"
        android:layout_height="72dp"
        android:background="@drawable/buttons"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/buttonProfileSetting"
        android:layout_width="209dp"
        android:layout_height="53dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/sfpro2"
        android:text="Modifica profilo"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.258" />

    <Button
        android:id="@+id/buttonPasswordSettings"
        android:layout_width="209dp"
        android:layout_height="53dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/sfpro2"
        android:text="Cambia password"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/buttonProfileSetting"
        app:layout_constraintEnd_toEndOf="@+id/buttonProfileSetting"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.404"
        app:layout_constraintHorizontal_bias="0.0" />

    <Button
        android:id="@+id/buttonLogoutSettings"
        android:layout_width="209dp"
        android:layout_height="53dp"
        android:background="@drawable/buttons"
        android:fontFamily="@font/sfpro2"
        android:text="Log out"
        android:paddingLeft="50dp"
        android:paddingRight="70dp"
        android:drawableStart="@drawable/logout"
        android:textAllCaps="false"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="@+id/buttonProfileSetting"
        app:layout_constraintEnd_toEndOf="@+id/buttonProfileSetting"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.548"
        app:layout_constraintHorizontal_bias="0.0" />

    <ImageView
        android:id="@+id/ChatS"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:src="@drawable/chatwhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.043"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/ProfileS"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:src="@drawable/profile"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.728"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/BachecaS"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:src="@drawable/boardwhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/SearchS"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:src="@drawable/searchwhite"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.262"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/settingsS"
        android:layout_width="42dp"
        android:layout_height="42dp"
        android:layout_marginBottom="16dp"
        android:clickable="true"
        android:src="@drawable/gear"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        tools:ignore="MissingConstraints" />

</androidx.constraintlayout.widget.ConstraintLayout>